{
	"name": "reading-plan-worker",
	"version": "1.0.0",
	"type": "module",
	"description": "Cloudflare worker TypeScript template",
	"scripts": {
		"dev": "npx wrangler dev",
		"publish": "npx wrangler publish",
		"format": "prettier --write '*.{json,js,ts}' 'src/**/*.{js,ts}' 'test/**/*.ts'",
		"lint": "eslint --max-warnings=0 src test && prettier --check '*.{json,js,ts}' 'src/**/*.{js,ts}' 'test/**/*.ts'",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"author": "Seth Alexander setha@hey.com",
	"license": "MIT",
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "0.12.1",
		"@cloudflare/workers-types": "4.20260108.0",
		"@eslint/js": "9.39.2",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"miniflare": "4.20260103.0",
		"prettier": "3.7.4",
		"typescript": "5.9.3",
		"typescript-eslint": "8.52.0",
		"vitest": "3.2.4",
		"wrangler": "4.57.0"
	}
}
