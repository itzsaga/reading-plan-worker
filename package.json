	"license": "MIT",
	"devDependencies": {
		"@cloudflare/workers-types": "4.20251230.0",
		"@eslint/js": "^9.0.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^10.0.0",
		"typescript-eslint": "^8.0.0",
		"miniflare": "^4.0.0",
		"prettier": "^3.0.0",
		"typescript": "5.9.3",
		"wrangler": "^4.0.0"
	}
}
