{
	"name": "reading-plan-worker",
	"version": "1.0.0",
	"type": "module",
	"description": "Cloudflare worker TypeScript template",
	"scripts": {
		"dev": "npx wrangler dev",
		"publish": "npx wrangler publish",
		"format": "prettier --write '*.{json,js}' 'src/**/*.{js,ts}'",
		"lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}'"
	},
	"author": "Seth Alexander setha@hey.com",
	"license": "MIT",
	"devDependencies": {
		"@cloudflare/workers-types": "4.20251231.0",
		"@eslint/js": "9.39.2",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"typescript-eslint": "8.51.0",
		"miniflare": "4.20251217.0",
		"prettier": "3.7.4",
		"typescript": "5.9.3",
		"wrangler": "4.54.0"
	}
}
